<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codesieucap.ueh_checkin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ueh_checkin">
        <activity
            android:name=".ui.loginAndRegister.ChangePasswordSuccessActivity"
            android:exported="false" />
        <activity
            android:name=".ui.loginAndRegister.FogetPassword3Activity"
            android:exported="false"
            android:label="@string/title_foget_password3" />
        <activity
            android:name=".ui.loginAndRegister.FogetPassword2Activity"
            android:exported="false"
            android:label="@string/title_foget_password2"
            android:parentActivityName=".ui.loginAndRegister.FogetPassword1Activity" />
        <activity
            android:name=".ui.loginAndRegister.FogetPassword1Activity"
            android:exported="false"
            android:label="@string/title_foget_password1"
            android:parentActivityName=".ui.loginAndRegister.LoginActivity" />
        <activity
            android:name=".ui.loginAndRegister.RegisterSuccessActivity"
            android:exported="false" />
        <activity
            android:name=".ui.loginAndRegister.PolicyActivity"
            android:exported="false" />
        <activity
            android:name=".ui.loginAndRegister.RegisterActivity"
            android:exported="false"
            android:label="@string/title_register"
            android:parentActivityName=".WelcomeActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".ui.loginAndRegister.LoginActivity"
            android:exported="false"
            android:label="@string/title_login"
            android:parentActivityName=".WelcomeActivity" />
        <activity
            android:name=".EventDetailActivity"
            android:exported="false" />
        <activity
            android:name=".WelcomeActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>